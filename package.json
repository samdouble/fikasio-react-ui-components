    "jest-image-snapshot": "^6.1.0",
    "puppeteer": "18.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.17",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4",
    "storybook": "^7.0.4",
    "typescript": "4.8.3",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17",
    "react-dom": ">=16.8.0 || ^17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{ts,tsx}",
      "!./src/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "./src/stories/",
      "./src/setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 15,
        "lines": 45,
        "statements": 45
      }
    }
  }
}
