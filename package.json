{
  "name": "@fikasio/react-ui-components",
  "description": "UI components for fikas products",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "samdouble",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/samdouble/fikasio-react-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/samdouble/fikasio-react-ui-components/issues"
  },
  "homepage": "https://github.com/samdouble/fikasio-react-ui-components#readme",
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "storybook build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx ./src/",
    "prepublish": "rm -rf ./dist && yarn build",
    "start": "react-scripts start",
    "storybook": "DISABLE_ESLINT_PLUGIN=true storybook dev -p 6006",
    "test": "react-scripts test --collectCoverage",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-ct:update": "playwright test -c playwright-ct.config.ts --update-snapshots"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^6.1.10",
    "bootstrap": "^5.3.2",
    "classnames": "^2.3.2",
    "font-awesome": "^4.7.0",
    "lodash.isequal": "^4.5.0",
    "luxon": "^3.4.3",
    "react-bootstrap": "^2.10.1",
    "react-click-outside-hook": "^1.1.1",
    "react-contenteditable": "^3.3.7",
    "react-datepicker": "8.8",
    "sass": "^1.56.1",
    "use-previous": "^1.2.0",
    "use-timeout": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chromatic-com/storybook": "^4.1.2",
    "@playwright/experimental-ct-react": "^1.49.1",
    "@playwright/test": "^1.49.0",
    "@rollup/plugin-image": "^3.0.2",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-links": "^10.0.6",
    "@storybook/preset-create-react-app": "^10.0.6",
    "@storybook/react-webpack5": "^10.0.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest-image-snapshot": "^6.2.1",
    "@types/lodash.isequal": "^4.5.8",
    "@types/luxon": "^3.7.1",
    "@types/node": "22",
    "@types/react": "^19.0.2",
    "@types/react-click-outside-hook": "^1.0.3",
    "@types/react-fontawesome": "^1.6.6",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "8.57",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.6",
    "jest-image-snapshot": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.17",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-uglify": "^6.0.4",
    "storybook": "^10.0.6",
    "typescript": "4.8.3",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17",
    "react-dom": ">=16.8.0 || ^17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "./src/stories/",
      "./src/setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 2,
        "functions": 9,
        "lines": 27,
        "statements": 27
      }
    }
  },
  "resolutions": {
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  }
}
