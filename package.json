    "classnames": "^2.3.2",
    "font-awesome": "^4.7.0",
    "luxon": "^3.4.3",
    "prop-types": "^15.8.1",
    "react-autocomplete-input": "^1.0.20",
    "react-click-outside": "^3.0.1",
    "react-contenteditable": "^3.3.7",
    "react-datepicker": "^4.20.0",
    "react-fontawesome": "^1.7.1",
    "sass": "^1.56.1",
    "use-timeout": "^1.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^3.0.2",
    "@storybook/addon-actions": "^7.0.4",
