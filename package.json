{
  "name": "@fikasio/react-ui-components",
  "description": "UI components for fikas products",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "samdouble",
  "version": "0.3.4",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/samdouble/fikasio-react-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/samdouble/fikasio-react-ui-components/issues"
  },
  "homepage": "https://github.com/samdouble/fikasio-react-ui-components#readme",
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "storybook build -s public",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx ./src/",
    "prepublish": "rm -rf ./dist && yarn build",
    "start": "react-scripts start",
    "storybook": "storybook dev -p 6006 -s public",
    "test": "react-scripts test --collectCoverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
